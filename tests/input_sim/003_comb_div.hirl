module full_adder {
	input async a;
	input async b;
	input async cin;
	output async cout;
	output async q;
}


impl full_adder {
	cout = (a & b) | (cin & (a ^ b));
	q = a ^ b ^ cin;
}

module div_cell {
	input async a;
	input async b;
	input async s;
	input async cin;
	output async cout;
	output async r;
}

impl div_cell {
	full_adder u_fa{
		a,
		b: !b,
		cin,
		cout,
		q: async fa_out // FIXME it was 'auto fa_out'
	};

	r = s ? fa_out : a;
}
