	},
	<start:@L> "bool" <end:@R> => TypeSpecifier::Bool{
		location:SourceSpan::new_between(start,end),
	},
	<start:@L> "bus" "<" <width:Expression> ">" <end:@R> => TypeSpecifier::Bus{
		width,
		location:SourceSpan::new_between(start,end),
	},
}
